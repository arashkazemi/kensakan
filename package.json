{
  "name": "kensakan",
  "version": "1.0.0",
  "description": "A tool for stepping through and inspecting javascript code without browser's inspector.",
  "repository": "github:arashkazemi/kensakan",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
