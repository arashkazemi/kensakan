    "esprima": "^4.0.1"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "jsdoc": "^3.6.11",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev": "^1.1.1"
  }
}
